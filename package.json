{
  "name": "axios-record-replay-adapter",
  "version": "1.1.1",
  "description": "Sit back, relax, and enjoy automatic mocking for axios HTTP requests",
  "repository": "https://github.com/bmealhouse/axios-record-replay-adapter",
  "author": "Brent Mealhouse <bmealhouse@gmail.com>",
  "license": "MIT",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/tsdx-basic.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "prettier": {
    "useTabs": false,
    "trailingComma": "es5",
    "singleQuote": false,
    "bracketSpacing": true
  },
  "size-limit": [
    {
      "path": "dist/axios-record-replay-adapter.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/axios-record-replay-adapter.esm.js",
      "limit": "10 KB"
    }
  ],
  "xo": {
    "prettier": true,
    "envs": [
      "jest"
    ],
    "rules": {
      "capitalized-comments": "off"
    }
  },
  "devDependencies": {
    "axios": "0.21.1",
    "husky": "4.3.7",
    "tsdx": "0.14.1",
    "tslib": "2.1.0",
    "typescript": "4.1.3"
  },
  "peerDependencies": {
    "axios": ">=0.19.1"
  },
  "scripts": {
    "release": "yarn lint && yarn test && npm publish",
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint src",
    "prepare": "tsdx build"
  }
}
